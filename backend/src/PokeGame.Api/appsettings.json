{
  "AllowedHosts": "*",
  "Api": {
    "Title": "Pok√©Game API",
    "Version": "0.1.0"
  },
  "Authentication": {
    "AccessToken": {
      "Type": "at+jwt",
      "LifetimeSeconds": 300
    }
  },
  "Cookies": {
    "RefreshToken": {
      "HttpOnly": true,
      "MaxAge": "7.00:00:00",
      "SameSite": "Strict",
      "Secure": true
    },
    "Session": {
      "SameSite": "Strict"
    }
  },
  "Krakenar": {
    "Realm": "pokegame"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
