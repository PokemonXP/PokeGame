{
  "AllowedHosts": "*",
  "Api": {
    "Title": "Pok√©Game API",
    "Version": "0.1.1"
  },
  "Authentication": {
    "AccessToken": {
      "Type": "at+jwt",
      "LifetimeSeconds": 300
    }
  },
  "Cookies": {
    "RefreshToken": {
      "HttpOnly": true,
      "MaxAge": "7.00:00:00",
      "SameSite": "None",
      "Secure": true
    },
    "Session": {
      "SameSite": "None"
    }
  },
  "Cors": {
    "AllowAnyOrigin": false,
    "AllowedOrigins": [],
    "AllowAnyMethod": false,
    "AllowedMethods": [ "DELETE", "GET", "PATCH", "POST", "PUT" ],
    "AllowAnyHeader": false,
    "AllowedHeaders": [ "Content-Type" ],
    "AllowCredentials": true
  },
  "Database": {
    "Provider": "EntityFrameworkCorePostgreSQL"
  },
  "Google": {
    "IdentifierKey": "Google"
  },
  "Krakenar": {
    "Realm": "pokegame"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDB": {
    "DatabaseName": "pokegame",
    "EnableLogging": true
  }
}
